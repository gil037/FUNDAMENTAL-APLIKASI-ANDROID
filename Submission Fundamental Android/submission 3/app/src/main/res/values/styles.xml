<resources>
    <style name="Widget.App.TextInputLayout" parent="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="endIconMode">clear_text</item>
        <item name="startIconTint">@color/primary_dark</item>
        <item name="endIconTint">@color/primary_dark</item>
        <item name="startIconDrawable">@drawable/ic_searching</item>
        <item name="placeholderText">@string/search</item>
        <item name="placeholderTextColor">@color/primary_dark</item>
    </style>

    <style name="Widget.Primary.MyButton" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@color/accent</item>
        <item name="cornerRadius">3dp</item>
        <item name="textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>